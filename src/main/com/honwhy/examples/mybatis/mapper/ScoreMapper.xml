<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.honwhy.examples.mybatis.mapper.ScoreMapper">
    <select id="getScoresByStudentId" resultType="com.honwhy.examples.mybatis.entity.Score">
        SELECT * FROM score WHERE student_id = #{studentId}
    </select>
</mapper>
